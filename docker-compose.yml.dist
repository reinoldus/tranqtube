version: '3'

services:
  nginx-videos:
    image: nginx:latest
    volumes:
      - ./deployment/nginx-videos/nginx.conf:/etc/nginx/nginx.conf:ro
      - ./deployment/nginx-videos/nginx_default.conf/:/etc/nginx/conf.d/default.conf:ro
    ports:
     - "44445:80"
  nginx-web:
    image: nginx:latest
    ports:
     - "44444:80"
    volumes:
      - ./deployment/nginx-web:/etc/nginx/conf.d/default.conf:ro
  tranqtube:
    build: .
  mongo:
    image: "mongo"
